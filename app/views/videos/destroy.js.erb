$("#alert-msgs").empty();
<% if @model.errors.any? %>
  $('#alert-msgs').prepend("<%= escape_javascript(render('utils/error_messages', errors: @model.errors)) %>");
<% else %>
  $('#alert-msgs').prepend("<%= escape_javascript(render('utils/success_messages')) %>");
  $("#photo-upload-videos").html('<%= escape_javascript(render("photos/video")) %>');
  <% flash.discard() %>
<% end %>
